{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'SignupView',\n  data() {\n    return {\n      email: '',\n      name: '',\n      nickname: '',\n      birthYear: '',\n      birthMonth: '',\n      birthDay: '',\n      gender: '',\n      selectedGenres: [],\n      // 선택된 장르들\n      isChecklistVisible: {\n        '총류': true,\n        '철학': false\n      },\n      // 체크리스트 표시 여부\n      genres: {\n        '총류': ['도서관, 서지학', '문헌정보학', '백과사전', '강연집, 수필집, 연설문집', '일반 연속간행물', '일반학회, 단체, 협회, 기관', '신문, 언론, 저널리즘', '일반전집, 총서', '향토자료'],\n        '철학': ['형이상학', '인식론, 인과론, 인간학', '철학의 체계', '경학', '아시아철학, 사상', '서양철학', '논리학', '심리학', '윤리학, 도덕철학']\n      }\n    };\n  },\n  computed: {\n    years() {\n      return Array.from({\n        length: 2025 - 1925 + 1\n      }, (_, i) => 1925 + i);\n    },\n    daysInMonth() {\n      return Array.from({\n        length: 31\n      }, (_, i) => i + 1);\n    }\n  },\n  methods: {\n    handleSignup() {\n      console.log('회원가입 시도:', {\n        email: this.email,\n        name: this.name,\n        nickname: this.nickname,\n        birthYear: this.birthYear,\n        birthMonth: this.birthMonth,\n        birthDay: this.birthDay,\n        gender: this.gender,\n        selectedGenres: this.selectedGenres // 선택된 장르 추가\n      });\n    },\n    goBack() {\n      this.$router.push('/login'); // 로그인 화면으로 이동\n    },\n    toggleChecklist(genre) {\n      this.isChecklistVisible[genre] = !this.isChecklistVisible[genre];\n    }\n  }\n};","map":{"version":3,"names":["name","data","email","nickname","birthYear","birthMonth","birthDay","gender","selectedGenres","isChecklistVisible","genres","computed","years","Array","from","length","_","i","daysInMonth","methods","handleSignup","console","log","goBack","$router","push","toggleChecklist","genre"],"sources":["C:\\Users\\sec\\Desktop\\Vue\\Last\\lib\\src\\views\\SignupView.vue"],"sourcesContent":["<template>\r\n    <div class=\"form-container\">\r\n      <!-- 뒤로가기 버튼 추가 -->\r\n      <button @click=\"goBack\" class=\"back-button\">◁</button>\r\n  \r\n      <h2>회원가입</h2>\r\n      <form>\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">이메일</label>\r\n          <input type=\"email\" id=\"email\" placeholder=\"이메일 입력\" />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"name\">이름</label>\r\n          <input type=\"text\" id=\"name\" placeholder=\"이름 입력\" />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"nickname\">닉네임</label>\r\n          <input type=\"text\" id=\"nickname\" placeholder=\"닉네임 입력\" />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label>생년월일</label>\r\n          <div class=\"birthdate\">\r\n            <select v-model=\"birthYear\" required>\r\n              <option value=\"\" disabled selected>연</option>\r\n              <option v-for=\"year in years\" :key=\"year\" :value=\"year\">{{ year }}</option>\r\n            </select>\r\n            <select v-model=\"birthMonth\" required>\r\n              <option value=\"\" disabled selected>월</option>\r\n              <option v-for=\"month in 12\" :key=\"month\" :value=\"month\">{{ month }}</option>\r\n            </select>\r\n            <select v-model=\"birthDay\" required>\r\n              <option value=\"\" disabled selected>일</option>\r\n              <option v-for=\"day in daysInMonth\" :key=\"day\" :value=\"day\">{{ day }}</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label>성별</label>\r\n          <div class=\"gender\">\r\n            <label>\r\n              <input type=\"radio\" value=\"남\" v-model=\"gender\" required /> 남\r\n            </label>\r\n            <label>\r\n              <input type=\"radio\" value=\"여\" v-model=\"gender\" required /> 여\r\n            </label>\r\n          </div>\r\n        </div>\r\n  \r\n        <!-- 선호 장르 추가 -->\r\n        <div class=\"form-group\">\r\n          <label>선호 장르</label>\r\n          <div class=\"genres\">\r\n            <div class=\"genre-item\" @click=\"toggleChecklist('총류')\">\r\n              <strong>총류</strong>\r\n              <div v-if=\"isChecklistVisible['총류']\" class=\"checklist\">\r\n                <label v-for=\"item in genres['총류']\" :key=\"item\">\r\n                  <input type=\"checkbox\" :value=\"item\" v-model=\"selectedGenres\" /> {{ item }}\r\n                </label>\r\n              </div>\r\n            </div>\r\n  \r\n            <div class=\"genre-item\" @click=\"toggleChecklist('철학')\">\r\n              <strong>철학</strong>\r\n              <div v-if=\"isChecklistVisible['철학']\" class=\"checklist\">\r\n                <label v-for=\"item in genres['철학']\" :key=\"item\">\r\n                  <input type=\"checkbox\" :value=\"item\" v-model=\"selectedGenres\" /> {{ item }}\r\n                </label>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n  \r\n        <button type=\"submit\" class=\"signup-button\">회원가입하기</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    name: 'SignupView',\r\n    data() {\r\n      return {\r\n        email: '',\r\n        name: '',\r\n        nickname: '',\r\n        birthYear: '',\r\n        birthMonth: '',\r\n        birthDay: '',\r\n        gender: '',\r\n        selectedGenres: [], // 선택된 장르들\r\n        isChecklistVisible: { '총류': true, '철학': false }, // 체크리스트 표시 여부\r\n        genres: {\r\n          '총류': [\r\n            '도서관, 서지학', '문헌정보학', '백과사전', '강연집, 수필집, 연설문집',\r\n            '일반 연속간행물', '일반학회, 단체, 협회, 기관', '신문, 언론, 저널리즘',\r\n            '일반전집, 총서', '향토자료'\r\n          ],\r\n          '철학': [\r\n            '형이상학', '인식론, 인과론, 인간학', '철학의 체계', '경학', '아시아철학, 사상',\r\n            '서양철학', '논리학', '심리학', '윤리학, 도덕철학'\r\n          ]\r\n        }\r\n      };\r\n    },\r\n    computed: {\r\n      years() {\r\n        return Array.from({ length: 2025 - 1925 + 1 }, (_, i) => 1925 + i);\r\n      },\r\n      daysInMonth() {\r\n        return Array.from({ length: 31 }, (_, i) => i + 1);\r\n      },\r\n    },\r\n    methods: {\r\n      handleSignup() {\r\n        console.log('회원가입 시도:', {\r\n          email: this.email,\r\n          name: this.name,\r\n          nickname: this.nickname,\r\n          birthYear: this.birthYear,\r\n          birthMonth: this.birthMonth,\r\n          birthDay: this.birthDay,\r\n          gender: this.gender,\r\n          selectedGenres: this.selectedGenres, // 선택된 장르 추가\r\n        });\r\n      },\r\n      goBack() {\r\n        this.$router.push('/login'); // 로그인 화면으로 이동\r\n      },\r\n      toggleChecklist(genre) {\r\n        this.isChecklistVisible[genre] = !this.isChecklistVisible[genre];\r\n      }\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .form-container {\r\n    max-width: 400px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    background: #fff;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  h2 {\r\n    text-align: center;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    margin-bottom: 8px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  input,\r\n  select {\r\n    width: 100%;\r\n    padding: 10px;\r\n    font-size: 14px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n    box-sizing: border-box;\r\n  }\r\n  \r\n  /* 생년월일 가로 정렬 */\r\n  .birthdate {\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n  \r\n  .birthdate select {\r\n    width: 32%; /* 세 칸으로 나누기 */\r\n  }\r\n  \r\n  /* 성별 체크박스 */\r\n  .gender {\r\n    display: flex;\r\n    justify-content: space-around;\r\n  }\r\n  \r\n  .gender label {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 7px; /* 텍스트와 체크박스 간격 */\r\n  }\r\n  \r\n  /* 선호 장르 */\r\n  .genres {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n  \r\n  .genre-item {\r\n    margin-bottom: 15px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .genre-item strong {\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    display: block;\r\n  }\r\n  \r\n  .checklist {\r\n    margin-top: 10px;\r\n    padding-left: 20px;\r\n  }\r\n  \r\n  .signup-button {\r\n    width: 100%;\r\n    padding: 12px;\r\n    font-size: 16px;\r\n    color: #fff;\r\n    background: #28a745;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s;\r\n  }\r\n  \r\n  .signup-button:hover {\r\n    background: #218838;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA8EE,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTF,IAAI,EAAE,EAAE;MACRG,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,cAAc,EAAE,EAAE;MAAE;MACpBC,kBAAkB,EAAE;QAAE,IAAI,EAAE,IAAI;QAAE,IAAI,EAAE;MAAM,CAAC;MAAE;MACjDC,MAAM,EAAE;QACN,IAAI,EAAE,CACJ,UAAU,EAAE,OAAO,EAAE,MAAM,EAAE,gBAAgB,EAC7C,UAAU,EAAE,kBAAkB,EAAE,cAAc,EAC9C,UAAU,EAAE,MAAK,CAClB;QACD,IAAI,EAAE,CACJ,MAAM,EAAE,eAAe,EAAE,QAAQ,EAAE,IAAI,EAAE,WAAW,EACpD,MAAM,EAAE,KAAK,EAAE,KAAK,EAAE,WAAU;MAEpC;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,KAAKA,CAAA,EAAG;MACN,OAAOC,KAAK,CAACC,IAAI,CAAC;QAAEC,MAAM,EAAE,IAAG,GAAI,IAAG,GAAI;MAAE,CAAC,EAAE,CAACC,CAAC,EAAEC,CAAC,KAAK,IAAG,GAAIA,CAAC,CAAC;IACpE,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,OAAOL,KAAK,CAACC,IAAI,CAAC;QAAEC,MAAM,EAAE;MAAG,CAAC,EAAE,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAA,GAAI,CAAC,CAAC;IACpD;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbC,OAAO,CAACC,GAAG,CAAC,UAAU,EAAE;QACtBpB,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBF,IAAI,EAAE,IAAI,CAACA,IAAI;QACfG,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBC,UAAU,EAAE,IAAI,CAACA,UAAU;QAC3BC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,cAAc,EAAE,IAAI,CAACA,cAAc,CAAE;MACvC,CAAC,CAAC;IACJ,CAAC;IACDe,MAAMA,CAAA,EAAG;MACP,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;IAC/B,CAAC;IACDC,eA