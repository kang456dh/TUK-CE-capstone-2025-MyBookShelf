{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from \"axios\"; // axios import Ï∂îÍ∞Ä\n\naxios.defaults.baseURL = 'http://localhost:8081'; // Spring Boot ÏÑúÎ≤Ñ Ï£ºÏÜå\n\nexport default {\n  name: \"SignupView\",\n  data() {\n    return {\n      email: \"\",\n      verificationCode: \"\",\n      password: \"\",\n      passwordConfirm: \"\",\n      passwordVisible: false,\n      passwordConfirmVisible: false,\n      realname: \"\",\n      nickname: \"\",\n      birthdate: \"\",\n      gender: \"\",\n      allGenres: [{\n        label: \"ÌåêÌÉÄÏßÄ\",\n        value: \"FANTASY\"\n      }, {\n        label: \"Í≥ºÌïô ÏÜåÏÑ§\",\n        value: \"SCIENCE_FICTION\"\n      }, {\n        label: \"ÎØ∏Ïä§ÌÑ∞Î¶¨\",\n        value: \"MYSTERY\"\n      }],\n      selectedGenre1: \"\",\n      selectedGenre2: \"\",\n      selectedGenre3: \"\",\n      emailErrorMessage: \"\",\n      verificationErrorMessage: \"\",\n      isVerificationCodeSent: false\n    };\n  },\n  computed: {\n    isPasswordMismatch() {\n      return this.passwordConfirm && this.password !== this.passwordConfirm;\n    }\n  },\n  methods: {\n    goBack() {\n      this.$router.push(\"/login\");\n    },\n    async checkEmailDuplicate() {\n      try {\n        const response = await axios.get(`/api/user/check-email?email=${encodeURIComponent(this.email)}`);\n        if (response.data.success) {\n          this.emailErrorMessage = \"ÏÇ¨Ïö© Í∞ÄÎä•Ìïú Ïù¥Î©îÏùºÏûÖÎãàÎã§.\";\n        } else {\n          /* this.emailErrorMessage = response.data.message; // ApiResponseÏóêÏÑú Î©îÏãúÏßÄ Í∞ÄÏ†∏Ïò§Í∏∞ */\n          this.emailErrorMessage = \"Ï§ëÎ≥µÎêú Ïù¥Î©îÏùºÏûÖÎãàÎã§.\";\n        }\n      } catch (error) {\n        console.error(\"Ïù¥Î©îÏùº Ï§ëÎ≥µ ÌôïÏù∏ Ïò§Î•ò:\", error);\n      }\n    },\n    async sendVerificationCode() {\n      if (!this.email) {\n        alert(\"Ïù¥Î©îÏùºÏùÑ ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      try {\n        const response = await axios.post(\"/api/email/send-verification\", {\n          email: this.email\n        });\n        if (response.data.isSuccess) {\n          alert(\"Ïù∏Ï¶ùÎ≤àÌò∏Í∞Ä Ïù¥Î©îÏùºÎ°ú Ï†ÑÏÜ°ÎêòÏóàÏäµÎãàÎã§.\");\n          this.isVerificationCodeSent = true;\n        } else {\n          alert(\"Ïù∏Ï¶ùÎ≤àÌò∏ Ï†ÑÏÜ°Ïóê Ïã§Ìå®ÌñàÏäµÎãàÎã§.\");\n        }\n      } catch (error) {\n        console.error(\"Ïù∏Ï¶ùÎ≤àÌò∏ Ï†ÑÏÜ° Ïò§Î•ò:\", error);\n      }\n    },\n    async verifyCode() {\n      if (!this.verificationCode) {\n        alert(\"Ïù∏Ï¶ùÎ≤àÌò∏Î•º ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      try {\n        const response = await axios.get(`/api/email/verify-code?inputCode=${this.verificationCode}`);\n        if (response.data.isSuccess) {\n          alert(\"Ïù∏Ï¶ùÎ≤àÌò∏Í∞Ä ÌôïÏù∏ÎêòÏóàÏäµÎãàÎã§.\");\n          // Ïù¥ÌõÑ ÌöåÏõêÍ∞ÄÏûÖ ÏßÑÌñâ ÎòêÎäî Îã§Ïùå Îã®Í≥ÑÎ°ú Ïù¥Îèô\n        } else {\n          alert(\"Ïù∏Ï¶ùÎ≤àÌò∏ ÌôïÏù∏Ïóê Ïã§Ìå®ÌñàÏäµÎãàÎã§: \" + response.data.message);\n        }\n      } catch (error) {\n        console.error(\"Ïù∏Ï¶ùÎ≤àÌò∏ Í≤ÄÏ¶ù Ïò§Î•ò:\", error);\n      }\n    },\n    async checkNicknameDuplicate() {\n      try {\n        const response = await axios.get(`/api/user/check-nickname?nickname=${encodeURIComponent(this.nickname)}`);\n        if (response.data.success) {\n          this.nicknameErrorMessage = \"ÏÇ¨Ïö© Í∞ÄÎä•Ìïú ÎãâÎÑ§ÏûÑÏûÖÎãàÎã§.\";\n        } else {\n          this.nicknameErrorMessage = \"Ï§ëÎ≥µÎêú ÎãâÎÑ§ÏûÑÏûÖÎãàÎã§.\"; // API Î©îÏãúÏßÄ ÏÇ¨Ïö©\n        }\n      } catch (error) {\n        console.error(\"ÎãâÎÑ§ÏûÑ Ï§ëÎ≥µ ÌôïÏù∏ Ïò§Î•ò:\", error);\n      }\n    },\n    async handleSignup() {\n      // ÏûÖÎ†•Í∞í Ïú†Ìö®ÏÑ± Í≤ÄÏ¶ù\n      if (!this.email) {\n        alert(\"Ïù¥Î©îÏùºÏùÑ ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (this.emailErrorMessage !== \"ÏÇ¨Ïö© Í∞ÄÎä•Ìïú Ïù¥Î©îÏùºÏûÖÎãàÎã§.\") {\n        alert(\"Ïù¥Î©îÏùº Ï§ëÎ≥µ ÌôïÏù∏ÏùÑ Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (!this.isVerificationCodeSent || !this.verificationCode) {\n        alert(\"Ïù∏Ï¶ùÎ≤àÌò∏Î•º ÏûÖÎ†•ÌïòÍ≥† Ïù∏Ï¶ùÏùÑ ÏôÑÎ£åÌï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (!this.password || !this.passwordConfirm) {\n        alert(\"ÎπÑÎ∞ÄÎ≤àÌò∏Î•º ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (this.password !== this.passwordConfirm) {\n        alert(\"ÎπÑÎ∞ÄÎ≤àÌò∏Í∞Ä ÏùºÏπòÌïòÏßÄ ÏïäÏäµÎãàÎã§.\");\n        return;\n      }\n      if (!this.realname) {\n        alert(\"Ïù¥Î¶ÑÏùÑ ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (!this.nickname) {\n        alert(\"ÎãâÎÑ§ÏûÑÏùÑ ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (this.nicknameErrorMessage !== \"ÏÇ¨Ïö© Í∞ÄÎä•Ìïú ÎãâÎÑ§ÏûÑÏûÖÎãàÎã§.\") {\n        alert(\"ÎãâÎÑ§ÏûÑ Ï§ëÎ≥µ ÌôïÏù∏ÏùÑ Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (!this.birthdate) {\n        alert(\"ÏÉùÎÖÑÏõîÏùºÏùÑ ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (!this.gender) {\n        alert(\"ÏÑ±Î≥ÑÏùÑ ÏÑ†ÌÉùÌï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n      if (!this.selectedGenre1) {\n        alert(\"ÏµúÏÜå Ìïú Í∞úÏùò Ïû•Î•¥Î•º ÏÑ†ÌÉùÌï¥Ï£ºÏÑ∏Ïöî.\");\n        return;\n      }\n\n      // ÌöåÏõêÍ∞ÄÏûÖ ÏöîÏ≤≠ Îç∞Ïù¥ÌÑ∞\n      const signupData = {\n        email: this.email,\n        password: this.password,\n        passwordConfirm: this.passwordConfirm,\n        realname: this.realname,\n        nickname: this.nickname,\n        birthDate: this.birthdate,\n        gender: this.gender,\n        genre: [this.selectedGenre1, this.selectedGenre2, this.selectedGenre3].filter(g => g)\n      };\n      try {\n        const response = await axios.post(\"/api/user/register\", signupData);\n        if (response.data.success) {\n          alert(\"ÌöåÏõêÍ∞ÄÏûÖ ÏÑ±Í≥µ!\");\n          this.$router.push(\"/login\"); // ÏÑ±Í≥µ Ïãú Î°úÍ∑∏Ïù∏ ÌéòÏù¥ÏßÄÎ°ú Ïù¥Îèô\n        } else {\n          alert(\"ÌöåÏõêÍ∞ÄÏûÖ Ïã§Ìå®: \" + response.data.message);\n        }\n      } catch (error) {\n        console.error(\"ÌöåÏõêÍ∞ÄÏûÖ Ïò§Î•ò:\", error.response ? error.response.data : error.message);\n        alert(\"ÌöåÏõêÍ∞ÄÏûÖ Ïò§Î•ò: \" + (error.response ? error.response.data.message : \"Ïïå Ïàò ÏóÜÎäî Ïò§Î•òÏûÖÎãàÎã§.\"));\n      }\n    },\n    togglePasswordVisibility(field) {\n      if (field === \"password\") {\n        this.passwordVisible = !this.passwordVisible;\n      } else if (field === \"passwordConfirm\") {\n        this.passwordConfirmVisible = !this.passwordConfirmVisible;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","defaults","baseURL","name","data","email","verificationCode","password","passwordConfirm","passwordVisible","passwordConfirmVisible","realname","nickname","birthdate","gender","allGenres","label","value","selectedGenre1","selectedGenre2","selectedGenre3","emailErrorMessage","verificationErrorMessage","isVerificationCodeSent","computed","isPasswordMismatch","methods","goBack","$router","push","checkEmailDuplicate","response","get","encodeURIComponent","success","error","console","sendVerificationCode","alert","post","isSuccess","verifyCode","message","checkNicknameDuplicate","nicknameErrorMessage","handleSignup","signupData","birthDate","genre","filter","g","togglePasswordVisibility","field"],"sources":["C:\\Users\\sec\\Desktop\\Vue\\proj\\FE-MyBookShelf\\src\\views\\SignupView.vue"],"sourcesContent":["<template>\r\n  <div class=\"form-container\">\r\n    <button @click=\"goBack\" class=\"back-button\">‚óÅ</button>\r\n\r\n    <h2>ÌöåÏõêÍ∞ÄÏûÖ</h2>\r\n\r\n    <form @submit.prevent=\"handleSignup\">\r\n      <!-- Ïù¥Î©îÏùº -->\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">Ïù¥Î©îÏùº</label>\r\n        <div class=\"email-input-wrapper\">\r\n          <input \r\n            type=\"email\" \r\n            id=\"email\" \r\n            v-model=\"email\" \r\n            placeholder=\"Ïù¥Î©îÏùº ÏûÖÎ†•\" \r\n            required \r\n          />\r\n          <button type=\"button\" @click=\"checkEmailDuplicate\">Ï§ëÎ≥µ ÌôïÏù∏</button>\r\n        </div>\r\n        <span v-if=\"emailErrorMessage\" style=\"color: red;\">{{ emailErrorMessage }}</span>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"verificationCode\">Ïù∏Ï¶ùÎ≤àÌò∏</label>\r\n        <div class=\"email-input-wrapper\">\r\n          <input \r\n            type=\"text\" \r\n            id=\"verificationCode\" \r\n            v-model=\"verificationCode\" \r\n            placeholder=\"Ïù∏Ï¶ùÎ≤àÌò∏ ÏûÖÎ†•\" \r\n            required \r\n          />\r\n          <button type=\"button\" @click=\"sendVerificationCode\">Ïù∏Ï¶ùÎ≤àÌò∏ Î∞õÍ∏∞</button>\r\n          <button type=\"button\" @click=\"verifyCode\">Ïù∏Ï¶ùÌïòÍ∏∞</button>\r\n        </div>\r\n        <span v-if=\"verificationErrorMessage\" style=\"color: red;\">{{ verificationErrorMessage }}</span>\r\n      </div>\r\n\r\n      <!-- ÎπÑÎ∞ÄÎ≤àÌò∏ -->\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">ÎπÑÎ∞ÄÎ≤àÌò∏</label>\r\n        <div class=\"password-input-wrapper\">\r\n          <input\r\n            :type=\"passwordVisible ? 'text' : 'password'\"\r\n            id=\"password\"\r\n            v-model=\"password\"\r\n            placeholder=\"ÎπÑÎ∞ÄÎ≤àÌò∏ ÏûÖÎ†•\"\r\n            required\r\n          />\r\n          <span class=\"toggle-visibility\" @click=\"togglePasswordVisibility('password')\">\r\n            {{ passwordVisible ? 'üëÅÔ∏è' : 'üëÅÔ∏è‚Äçüó®Ô∏è' }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    \r\n      <!-- ÎπÑÎ∞ÄÎ≤àÌò∏ ÌôïÏù∏ -->\r\n      <div class=\"form-group\">\r\n        <label for=\"passwordConfirm\">ÎπÑÎ∞ÄÎ≤àÌò∏ ÌôïÏù∏</label>\r\n        <div class=\"password-input-wrapper\">\r\n          <input\r\n            :type=\"passwordConfirmVisible ? 'text' : 'password'\"\r\n            id=\"passwordConfirm\"\r\n            v-model=\"passwordConfirm\"\r\n            :class=\"{ 'input-error': isPasswordMismatch }\"\r\n            placeholder=\"ÎπÑÎ∞ÄÎ≤àÌò∏ ÌôïÏù∏ ÏûÖÎ†•\"\r\n            required\r\n          />\r\n          <span class=\"toggle-visibility\" @click=\"togglePasswordVisibility('passwordConfirm')\">\r\n            {{ passwordConfirmVisible ? 'üëÅÔ∏è' : 'üëÅÔ∏è‚Äçüó®Ô∏è' }}\r\n          </span>\r\n        </div>\r\n        <span v-if=\"isPasswordMismatch\" style=\"color: red;\">ÎπÑÎ∞ÄÎ≤àÌò∏Í∞Ä ÏùºÏπòÌïòÏßÄ ÏïäÏäµÎãàÎã§.</span>\r\n      </div>\r\n\r\n\r\n      <!-- Ïù¥Î¶Ñ -->\r\n      <div class=\"form-group\">\r\n        <label for=\"realname\">Ïù¥Î¶Ñ</label>\r\n        <input type=\"text\" id=\"realname\" v-model=\"realname\" placeholder=\"Ïù¥Î¶Ñ ÏûÖÎ†•\" required />\r\n      </div>\r\n\r\n      <!-- ÎãâÎÑ§ÏûÑ -->\r\n      <div class=\"form-group\">\r\n        <label for=\"nickname\">ÎãâÎÑ§ÏûÑ</label>\r\n        <div class=\"email-input-wrapper\">\r\n          <input type=\"text\" id=\"nickname\" v-model=\"nickname\" placeholder=\"ÎãâÎÑ§ÏûÑ ÏûÖÎ†•\" required />\r\n          <button type=\"button\" @click=\"checkNicknameDuplicate\">Ï§ëÎ≥µ ÌôïÏù∏</button>\r\n        </div>\r\n        <span v-if=\"nicknameErrorMessage\" style=\"color: red;\">{{ nicknameErrorMessage }}</span>\r\n      </div>\r\n\r\n      <!-- ÏÉùÎÖÑÏõîÏùº -->\r\n      <div class=\"form-group\">\r\n        <label for=\"birthdate\">ÏÉùÎÖÑÏõîÏùº</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"birthdate\"\r\n          v-model=\"birthdate\"\r\n          placeholder=\"YYYY-MM-DD\"\r\n          pattern=\"\\d{4}-\\d{2}-\\d{2}\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <!-- ÏÑ±Î≥Ñ -->\r\n      <div class=\"form-group\">\r\n        <label>ÏÑ±Î≥Ñ</label>\r\n        <div class=\"gender\">\r\n          <label>\r\n            <input type=\"radio\" value=\"MALE\" v-model=\"gender\" required />\r\n            ÎÇ®ÏÑ±\r\n          </label>\r\n          <label>\r\n            <input type=\"radio\" value=\"FEMALE\" v-model=\"gender\" required />\r\n            Ïó¨ÏÑ±\r\n          </label>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ÏÑ†Ìò∏ Ïû•Î•¥ -->\r\n      <div class=\"form-group\">\r\n        <label>ÏÑ†Ìò∏ Ïû•Î•¥(ÏµúÏÜå 1Í∞ú ÏÑ†ÌÉù)</label>\r\n        <div class=\"genre-select\">\r\n          <label for=\"genre1\">-Ïû•Î•¥ 1-</label>\r\n          <select v-model=\"selectedGenre1\" id=\"genre1\" required>\r\n            <option value=\"\" disabled>ÏÑ†ÌÉù</option>\r\n            <option v-for=\"genre in allGenres\" :key=\"genre.value\" :value=\"genre.value\">\r\n              {{ genre.label }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n        <div class=\"genre-select\">\r\n          <label for=\"genre2\">-Ïû•Î•¥ 2-</label>\r\n          <select v-model=\"selectedGenre2\" id=\"genre2\">\r\n            <option value=\"\" disabled>ÏÑ†ÌÉù</option>\r\n            <option v-for=\"genre in allGenres\" :key=\"genre.value\" :value=\"genre.value\">\r\n              {{ genre.label }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n        <div class=\"genre-select\">\r\n          <label for=\"genre3\">-Ïû•Î•¥ 3-</label>\r\n          <select v-model=\"selectedGenre3\" id=\"genre3\">\r\n            <option value=\"\" disabled>ÏÑ†ÌÉù</option>\r\n            <option v-for=\"genre in allGenres\" :key=\"genre.value\" :value=\"genre.value\">\r\n              {{ genre.label }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n      </div>\r\n      <button type=\"submit\" class=\"signup-button\">ÌöåÏõêÍ∞ÄÏûÖÌïòÍ∏∞</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\"; // axios import Ï∂îÍ∞Ä\r\n\r\naxios.defaults.baseURL = 'http://localhost:8081'; // Spring Boot ÏÑúÎ≤Ñ Ï£ºÏÜå\r\n\r\nexport default {\r\n  name: \"SignupView\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      verificationCode: \"\",\r\n      password: \"\",\r\n      passwordConfirm: \"\",\r\n      passwordVisible: false,\r\n      passwordConfirmV